                <input type="search" name="q" id="q" required>
                <button> <i class="fa fa-search"></i> </button>
            </form>
            <label id="openMenu"> <i class="fa fa-bars"></i> </label>
        </nav>
    </header>
    <script src="https://kit.fontawesome.com/797817b03b.js" crossorigin="anonymous"></script>
    <script src="/js/script.js"></script>
